{
    "manifest_version": 3,
    "name": "Color Picker",
    "version": "1.0",
    "description": "A simple color picker extension for Google Chrome",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "permissions": [
      "activeTab"
    ]
  }
  